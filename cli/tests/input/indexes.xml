    <front>
        <title>xml2rfc index tests</title>
        <author fullname="Human Person" initials="H" surname="Person" role="editor">
            <address>
                <postal>
                    <postalLine>line 1</postalLine>
                    <postalLine>line 2</postalLine>
                    <postalLine>line 3</postalLine>
                </postal>
            </address>
        </author>
    </front>
    <middle>
        <section>
            <name>First Section</name>
            <iref item="section"/>
            <t>
                <iref item="t"/>
                This is a section!
                <em>
                    <iref item="em"/>
                    Emphatically.
                </em>
                <strong>
                    <iref item="strong"/>
                    Loudly.
                </strong>
                <sub>
                    <iref item="sub"/>
                    Subscripted.
                </sub>
                <sup>
                    <iref item="sup"/>
                    Superscripted.
                </sup>
                <tt>
                    <iref item="tt"/>
                    Typewritten.
                </tt>
            </t>
            <t>
                <iref item="t"/>
                This is another section!
            </t>
            <dl>
                <dt><iref item="dt"/>dt
                </dt>
                <dd>
                    <iref item="dd"/>
                    dd
                </dd>
            </dl>
            <ul>
                <li>
                    <iref item="li" subitem="ul"/>
                    unnumbered item
                </li>
            </ul>
            <ol>
                <li>
                    <iref item="li" subitem="ol"/>
                    numbered item
                </li>
            </ol>
            <aside>
                <!-- <iref> as a direct child of <aside> breaks the TextWriter -->
                <!-- <iref item="aside"/>-->
                <t>
                    <iref item="t" subitem="in an aside tag"/>
                    This is an aside.
                </t>
            </aside>
            <blockquote>
                <iref item="blockquote"/>
                This is a blockquote.
            </blockquote>
            <table>
                <iref item="table"/>
                <tbody>
                    <tr>
                        <th>
                            <iref item="th"/>
                            Table heading
                        </th>
                    </tr>
                    <tr>
                        <td>
                            <iref item="td"/>
                            <figure>
                                <iref item="figure"/>
                                <sourcecode>
                                    printf("Hello world!\n");
                                </sourcecode>
                            </figure>
                        </td>
                    </tr>
                </tbody>
            </table>
        </section>
    </middle>
    <back>
        <references>
            <name>References</name>
            <reference anchor="ref0">
                <front>
                    <title>Reference</title>
                    <author fullname="Reference Q. Author"/>
